{
  "name": "generator-atom-package-typescript",
  "version": "0.16.0",
  "description": "Generator for Atom packages written in TypeScript",
  "files": [
    "generators"
  ],
  "scripts": {
    "lint:ejs": "ejslint generators/**/*.ejs",
    "lint:js": "eslint generators/**/*.js",
    "lint": "concurrently 'npm run lint:js' 'npm run lint:ejs'",
    "test": "npm run lint"
  },
  "keywords": [
    "yeoman-generator",
    "atom",
    "atom package",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/generator-atom-package-typescript"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@sindresorhus/slugify": "^0.9.1",
    "axios": "^0.19.0",
    "change-case": "^3.1.0",
    "spdx-license-list": "^6.1.0",
    "terminal-link": "^2.0.0",
    "update-notifier": "^3.0.1",
    "yeoman-generator": "^4.1.0",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "concurrently": "^5.0.0",
    "ejs-lint": "^0.3.0",
    "eslint": "^6.5.0",
    "husky": "^3.0.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  }
}
